			"name": "commandline"
		}
		"runmode":{
			"name":"files",
			"configfile":"",
			"paramfile":""
		},
    	"csvfileprefix": "csv_output",
		"csvfiledir": "csvfiles"
	},
    "run":{
        "number_parameters":"2",
        "seed": "1111",
        "distribution_init_points": "uniform",
        "type":"optimization",
        "algorithm":[
    		{
    			"name":"GA",
        		"steps": "3", 
        		"maximum": "False",
				"restart": "false",
