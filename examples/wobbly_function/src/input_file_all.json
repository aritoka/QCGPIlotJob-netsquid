        		}
    		},
        	{
            	"name": "random",
            	"steps": "4",
    			"restart": "true",
        		"maximum": "False"
        	}]
    },
    "parameters":[
    {
        "Parameter": "1",
        "name":"x",
        "min":"-4",
        "max":"4",
        "number_points":"2",
